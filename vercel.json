{
  "version": 2,
  "builds": [
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "formulario-admins/**",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/formulario-admins",
      "dest": "/formulario-admins/index.html"
    },
    {
      "src": "/formulario-admins/(.*)",
      "dest": "/formulario-admins/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
